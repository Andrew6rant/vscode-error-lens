{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2015",
		"outDir": "out",
		"suppressImplicitAnyIndexErrors": true,
		"lib": [
			"ES2015",
			"ES2016"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
